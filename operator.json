{
  "name": "GSEA / ORA",
  "description": "Tests whether some gene sets are significantly enriched for a given outcome using Gene Set Enrichment Analysis or Over-Representation Analysis.",
  "tags": [
    "statistical testing",
    "omics"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/gsea_operator"
  ],
  "properties": [
    {
      "kind": "DoubleProperty",
      "name": "min_size",
      "defaultValue": 10,
      "description": "Minimal size of a gene set to be considered in the analysis (default: 10)"
    },
    {
      "kind": "DoubleProperty",
      "name": "max_size",
      "defaultValue": 500,
      "description": "Maximal size of a gene set to be considered in the analysis (default: 500)"
    },
    {
      "kind": "DoubleProperty",
      "name": "seed",
      "defaultValue": -1,
      "description": "Random seed for result reproducibility (default -1 will be ignored.)"
    },
    {
      "kind": "CategoricalProperty",
      "name": "method",
      "defaultValue": "GSEA",
      "description": "The analysis method to perform.",
      "values": [
        "GSEA",
        "ORA"
      ]
    }
  ],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Gene Set",
            "type": "",
            "description": "Gene set identifiers",
            "ontologyMapping": "gene set",
            "crosstabMapping": "row",
            "cardinality": "1..n",
            "factors": []
          },
          {
            "kind": "MetaFactor",
            "name": "Gene Name",
            "type": "",
            "description": "Gene name identifiers",
            "ontologyMapping": "gene name",
            "crosstabMapping": "column",
            "cardinality": "1..n",
            "factors": []
          }
        ],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "Measurement",
                "type": "numeric",
                "description": "Measurement values (e.g., normalized gene expression value)",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              }
            ]
          }
        ]
      }
    ]
  }
}